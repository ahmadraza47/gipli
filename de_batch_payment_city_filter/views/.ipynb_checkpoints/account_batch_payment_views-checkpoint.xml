<?xml version="1.0" encoding="utf-8"?>
<odoo>

   <record id="view_batch_payment_form_inherit" model="ir.ui.view">
        <field name='name'>view.batch.payment.form.inherit</field>
        <field name="model">account.batch.payment</field>
        <field name="inherit_id" ref="account_batch_payment.view_batch_payment_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='payment_method_id']" position="after">
                
                <field name="city_id" />
            </xpath>
            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='payment_ids']/form[1]/group/group" position="inside">
                <field name="city_id" invisible="1" />
            </xpath>
            
            <xpath expr="//field[@name='payment_ids']" position="attributes">
                <attribute name="context">"{'default_payment_type': batch_type, 'default_journal_id': journal_id, 'default_payment_method_id': payment_method_id, 'default_city_id: city_id}"</attribute>
            </xpath>
           
             <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='payment_ids']/form[1]/group/group/field[@name='partner_id']" position="attributes">
                 <attribute name="domain">[('partner_id', 'ilike', ['abc','xyz'])]</attribute>
            </xpath>
           
        </field>
    </record>

</odoo>