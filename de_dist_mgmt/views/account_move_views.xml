<odoo>
  <data>
    <record id="view_move_form1_inherit" model="ir.ui.view">
        <field name="name">view.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='price_unit']" position="after">
                <field name="retail_price_unit" optional="show" />
                <field name="tax_base_amount" optional="show" />
            </xpath>
        </field>
    </record>
      
      <record id="view_move_line_form1_inherit" model="ir.ui.view">
        <field name="name">view.move.line.form.inherit</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_form"/>
        <field name="arch" type="xml">
            <field name="quantity" position="after">
                <field name="retail_price_unit" />
            </field>
        </field>
    </record>
      
      
  </data>
</odoo>